            <AnchorPane layoutY="85.0" prefHeight="315.0" prefWidth="600.0" style="-fx-background-color: #E6E6FA;">
               <children>
                  <TextField fx:id="welcomeLoginField" layoutX="154.0" layoutY="86.0" prefHeight="25.0" prefWidth="279.0" promptText="Login">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
                  <PasswordField fx:id="welcomePassField" layoutX="154.0" layoutY="143.0" prefHeight="25.0" prefWidth="279.0" promptText="Password">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </PasswordField>
                  <Label layoutX="210.0" layoutY="31.0" prefHeight="26.0" prefWidth="206.0" text="Authorization" textFill="#171b46">
